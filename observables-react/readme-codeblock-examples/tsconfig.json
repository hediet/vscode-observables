{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "noEmit": true,
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "rootDir": "./out",
    "paths": {
      "@vscode/observables": ["../node_modules/@vscode/observables/dist/index.d.ts"],
      "@vscode/observables-react": ["../dist/index.d.ts"]
    }
  },
  "include": [
    "out/**/*.ts",
    "out/**/*.tsx"
  ],
  "exclude": [
    "out/**/*.playwright-spec.tsx"
  ]
}
