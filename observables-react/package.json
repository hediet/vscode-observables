{
    "name": "@vscode/observables-react",
    "description": "React bindings for VS Code observables",
    "authors": "Microsoft",
    "version": "1.0.0",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -b",
        "dev": "tsc -b --watch",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@vscode/observables": "*"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "typescript": "^5.0.0",
        "rimraf": "^5.0.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    }
}