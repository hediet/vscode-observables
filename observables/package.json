{
    "name": "@vscode/observables",
    "description": "Observable utilities for VS Code",
    "authors": "Microsoft",
    "version": "0.1.0",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./min": {
            "import": "./dist/index.min.js",
            "default": "./dist/index.min.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "prepare": "pnpm run build",
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^16.0.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.3.0",
        "rimraf": "^5.0.0",
        "rollup": "^3.29.5",
        "tslib": "^2.8.1",
        "typescript": "^5.0.0"
    }
}
