{
    "name": "@vscode/observables",
    "description": "Observable utilities for VS Code",
    "authors": "Microsoft",
    "version": "1.0.0",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -b",
        "dev": "tsc -b --watch",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "typescript": "^5.0.0",
        "rimraf": "^5.0.0"
    }
}